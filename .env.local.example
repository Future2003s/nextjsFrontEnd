# Frontend environment variables
# Update and copy to .env.local

# Public site URL (used in metadata, redirects, etc.)
NEXT_PUBLIC_URL=http://localhost:3000

# Backend API base URL (nodejsBackEnd)
# Example for local dev: http://localhost:8080/v1/api
NEXT_PUBLIC_API_END_POINT=http://localhost:8080/v1/api

# New Backend API Configuration (nodejsBackEnd)
# Backend server URL (without /api/v1)
NEXT_PUBLIC_BACKEND_URL=http://localhost:8081

# API version
NEXT_PUBLIC_API_VERSION=v1

# Logo URL used by the app header/footer
NEXT_PUBLIC_URL_LOGO=/images/logo.png
